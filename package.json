{
  "name": "@dljsbook/ui",
  "version": "0.10.1",
  "description": "This is a supporting library for the book Deep Learning With Javascript (https://dljsbook.com).",
  "author": "Kevin Scott <kevin@dljsbook.com>",
  "license": "MIT",
  "repository": "https://github.com/dljsbook/ui",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:typescript": "tsc -p tsconfig.json",
    "build:webpack": "webpack --config webpack.config.js",
    "build": "yarn clean && yarn lint && yarn build:typescript && yarn build:webpack",
    "clean": "rimraf dist",
    "develop": "tsc -p tsconfig.json --watch",
    "lint": "tslint -p . -t verbose",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prepublish": "yarn build",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@tensorflow/tfjs": "^0.14.2",
    "d3": "^5.8.0",
    "emotion": "^10.0.4",
    "pixelated-canvas": "^0.5.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^8.0.4",
    "roughjs": "^3.0.0",
    "styled-components": "^4.1.2",
    "vega-embed": "^3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@storybook/addon-info": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "@tensorflow/tfjs": "^0.14.2",
    "@types/d3": "^5.7.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.10",
    "@types/react-dom": "^16.0.11",
    "@types/react-dropzone": "^4.2.2",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "jest": "^23.6.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.2.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
