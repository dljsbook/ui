{
  "name": "@dljsbook/ui",
  "version": "0.7.15",
  "description": "",
  "repository": "https://github.com/dljsbook/UI",
  "main": "dist/index.js",
  "watch": {
    "build": "{src}/*.ts"
  },
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "tslint -p . -t verbose",
    "deploy": "gh-pages -d data",
    "prepublish": "yarn build",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn lint && tsc -p tsconfig.json",
    "develop": "tsc -p tsconfig.json --watch",
    "test": "jest --watchAll"
  },
  "author": "Kevin Scott <kevin@hitheory.com>",
  "license": "MIT",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@storybook/addon-info": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.10",
    "@types/react-dom": "^16.0.11",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "jest": "^23.6.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-npm": "^2.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@tensorflow/tfjs": "^0.14.1",
    "@tensorflow/tfjs-vis": "^0.3.0",
    "@types/color": "^3.0.0",
    "color": "^3.1.0",
    "d3-array": "^2.0.2",
    "d3-contour": "^1.3.2",
    "d3-geo": "^1.11.3",
    "d3-scale": "^2.1.2",
    "emotion": "^10.0.4",
    "pixelated-canvas": "^0.5.4",
    "react-dom": "^16.6.3",
    "react": "^16.6.3",
    "react-vega": "^4.0.2",
    "styled-components": "^4.1.2",
    "vega": "^4.4.0",
    "vega-embed": "^3.26.0",
    "vega-lib": "^4.3.0",
    "vega-lite": "^3.0.0-rc10",
    "vega-parser": "^3.9.0",
    "vega-view": "^3.4.1"
  }
}
